<?xml version="1.0" encoding="UTF-8"?>
<policy>
    <general>
        <name>Update Zoom</name>
        <enabled>true</enabled>
        <frequency>Ongoing</frequency>
        <category>
            <name>1-Monthly Patching</name>
        </category>
    </general>
    <package_configuration>
        <packages>
            <size>1</size>
            <package>
                <name>%NAME%.pkg</name>
                <action>Install</action>
            </package>
        </packages>
    </package_configuration>
    <scripts>
        <size>0</size>
    </scripts>
    <maintenance>
        <recon>true</recon>
    </maintenance>
</policy>
