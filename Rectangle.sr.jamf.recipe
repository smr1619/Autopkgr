<?xml version="1.0" encoding="UTF-8"?>
<recipe>
    <identifier>com.github.smr1619.Rectangle.sr.jamf.recipe</identifier>
    <description>Downloads the latest version of Rectangle from GitHub.</description>
    <input>
        <key>NAME</key>
        <string>Rectangle</string>
        <key>REPO_URL</key>
        <string>https://github.com/rxhanson/Rectangle</string>
        <key>CATEGORY</key>
        <string>Autopkgr-Files</string>
    </input>
    <processor>downloadURLFromGit</processor>
    <processor>unarchive</processor>
    <processor>renamePath</processor>
    <processor>versioner</processor>
    <processor>AppPkgCreator</processor>
    <processor>
        <key>Arguments</key>
        <dict>
            <key>category_name</key>
            <string>%CATEGORY%</string>
        </dict>
        <key>Processor</key>
        <string>com.github.grahampugh.jamf-upload.processors/JamfCategoryUploader</string>
    </processor>
    <processor>
        <key>Arguments</key>
        <dict>
            <key>pkg_category</key>
            <string>%CATEGORY%</string>
        </dict>
        <key>Processor</key>
        <string>com.github.grahampugh.jamf-upload.processors/JamfPackageUploader</string>
    </processor>
</recipe>
