<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Identifier</key>
    <string>com.example.boxdrive</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>Box Drive</string>
        <key>VERSION</key>
        <string>latest</string>
        <key>PKGID</key>
        <string>com.box.pkg.boxdrive</string>
        <key>URL</key>
        <string>https://api.box.com/2.0/files/FILE_ID/content</string>
        <key>POLICY_ID</key>
        <string>1</string>
    </dict>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>download</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%URL%</string>
                <key>pkgid</key>
                <string>%PKGID%</string>
                <key>output_path</key>
                <string>Box.pkg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>com.github.grahampugh.jamf-upload.processors/JamfPackageUploader</string>
            <key>Arguments</key>
            <dict>
                <key>prod_name</key>
                <string>%NAME%</string>
                <key>pkg_path</key>
                <string>Box.pkg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>com.github.grahampugh.jamf-upload.processors/JamfPolicyUploader</string>
            <key>Arguments</key>
            <dict>
                <key>policy_id</key>
                <string>%POLICY_ID%</string>
                <key>category</key>
                <string>1-Monthly Patching</string>
                <key>pkg_name</key>
                <string>%NAME%</string>
            </dict>
        </dict>
    </array>
</dict>
